{
  "name": "chromeHistoryFuzz",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Fuzzy search across all you history",
  "options_page": "options.html",
  "permissions": [
    "history",
    "storage",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+K",
        "mac": "Command+K",
        "linux": "Ctrl+K"
      }
    }
  },
  "browser_action": {
    "default_popup": "main.html",
    "default_icon": "H4.png"
  }
}
