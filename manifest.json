{
    "name": "chromeHistoryFuzz",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Fuzzy search across all you history",
    "permissions": [
        "history",
        "storage",
        "chrome://favicon/"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+L",
                "mac": "Alt+L"
            }
        }
    },
    "browser_action": {
        "default_popup": "main.html",
        "default_icon": "clock.png"
    }
}
